{
  "name": "poi-playground",
  "version": "1.0.0",
  "main": "app.tsx",
  "license": "MIT",
  "scripts": {
    "start": "poi --config ./build/poi.config.js",
    "build": "poi build --config ./build/poi.config.js",
    "lint": "tslint --config build/tslint.json --project tsconfig.json --format stylish --fix --force",
    "stylelint": "stylelint \"src/**/*.scss\" --config build/.stylelintrc.json --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "deploy": "now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@poi/plugin-typescript": "10.2.0",
    "@types/classnames": "2.2.6",
    "@types/node": "10.12.12",
    "@types/react": "16.7.11",
    "@types/react-dom": "16.0.11",
    "@types/react-transition-group": "2.0.14",
    "@types/yup": "0.26.2",
    "autoprefixer": "9.3.1",
    "classnames": "2.2.6",
    "chalk": "1.1.3",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.7",
    "dotenv": "6.1.0",
    "formik": "1.4.0",
    "husky": "1.2.0",
    "include-media": "1.4.9",
    "lint-staged": "8.1.0",
    "modularscale-sass": "3.0.8",
    "node-sass": "4.10.0",
    "now": "12.1.9",
    "object-assign": "4.1.1",
    "poi": "10.2.11",
    "postcss-bubbly-grid": "1.0.5",
    "postcss-pxtorem": "4.0.1",
    "prettier": "1.15.3",
    "promise-polyfill": "8.1.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-transition-group": "2.5.0",
    "sass-loader": "7.1.0",
    "space-lift": "0.8.7",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-idiomatic-order": "6.2.0",
    "stylelint-order": "2.0.0",
    "stylelint-webpack-plugin": "0.10.5",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.2.1",
    "unstated": "2.1.1",
    "yup": "0.26.6"
  }
}
